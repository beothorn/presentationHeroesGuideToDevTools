{
    "description": "SimpleExtension",
    "manifest_version": 2,
    "name": "SimpleExtension",
    "version": "0.0.1",
    "author": "You",
    "icons": {
        "48": "icons/logo-48.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "js": ["inject.js"]
        }
    ],
    "permissions": [
        "<all_urls>"
    ],
    "web_accessible_resources":["commands.js"]
}
